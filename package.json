{
  "name": "astrofy",
  "description": "Astrofy is a free and open-source template for your Personal Portfolio Website built with Astro and TailwindCSS. Create in minutes a website with Blog, CV, Project Section, Store and RSS Feed.",
  "type": "module",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run copy-images && astro dev",
    "start": "astro dev",
    "build": "npm run copy-images && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "copy-images": "pwsh scripts/copy-images.ps1",
    "copy-images:ci": "node scripts/copy-images.js"
  },
  "dependencies": {
    "@astrojs/mdx": "^2.0.3",
    "@astrojs/rss": "^3.0.0",
    "@astrojs/sitemap": "^3.0.1",
    "@astrojs/tailwind": "^5.0.3",
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/lucide": "^1.2.59",
    "astro": "^4.0.2",
    "astro-icon": "^1.1.5",
    "daisyui": "^4.4.10",
    "dayjs": "^1.11.9",
    "lucide-astro": "^0.534.0",
    "sharp": "^0.32.6",
    "tailwindcss": "^3.3.5"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10"
  }
}
